{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.SignalR": "Debug"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "Jwt": {
    "Key": "your-super-secret-key-change-this-in-production-minimum-32-chars",
    "Issuer": "orchestrator",
    "Audience": "orchestrator-client"
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:5050"
      }
    }
  },
  "CentralIngress": {
    "Enabled": true,
    "BaseDomain": "vms.decloud.io",
    "CaddyAdminUrl": "http://localhost:2019",
    "AcmeEmail": "admin@decloud.io",
    "UseAcmeStaging": false,
    "DnsProvider": "cloudflare",
    "DnsApiToken": "YOUR_CLOUDFLARE_API_TOKEN",
    "DefaultTargetPort": 80,
    "SubdomainPattern": "{name}",
    "AutoRegisterOnStart": true,
    "AutoRemoveOnStop": true,
    "ProxyTimeoutSeconds": 30,
    "EnableWebSocket": true,
    "RateLimitPerMinute": 0
  }
}
